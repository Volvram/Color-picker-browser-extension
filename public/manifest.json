{
  "name": "Color Picker Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_icon.png"
  },
  "permissions": ["scripting"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "hello_icon.png"
  }
}